<!doctype html>
<html lang="en">
  <head>
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

   
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="/static/css/style.css">
    
    <title>Tech Pa_&_rmil</title>
  </head>
  <body class="back_ground">
    
    {{>navbar data=details}} 
    {{> slider slides=slides}}
 
     <section class="banner1_section py-5 banner_bg">
     <div class="container text-center">
      <div class="row">
         {{#each banners}}

        <div class="col-md-12">
          <h1>{{this.title}}</h1>
          <p style="font-size:20px;" >
           {{this.description}}
          </p>
          <a href="{{this.link}}">
        <button class="btn btn-info" > {{this.linktext}} </button>    
          </a>
      </div>
      </div>
      {{/each}}
      </div>
     </section>




     {{!-- services.section --}}
    <section id="services_section"   >
      <div class="container py-3">
        <h1 class="text-center service_header" style="color: aliceblue;">Our Services</h1>
       <div class="row mt-5 ">
      {{#each services}}


         <div class="col-md-4">
          <div class="card text-black card_bg  mb-3">
            <div class="card-body">
              <div class="container text-center py-3" >
                <i class="{{this.icon}} service_icon"></i>

              </div>
              <h4 class="card-title">{{this.title}}</h4>
              <p>{{this.description}}</p>
              <div class="hover_style">
                <a href="{{this.linkText}}" >{{this.link}}</a>
                 <a href="{{this.linkText}}" class="ml-5"  >{{this.link2}}</a>
              </div>
              
                 
               
           
            </div>
          </div>
         </div>
   {{/each}}
       </div>


    </section>

     <section class="banner2_section py-5">
      <div class="container">
        <div class="row ">
          {{#each bannere}}
          <div class="col-md-6 text-center pt-5">
            <h2>
            {{this.title1}}
            </h2>
            <p style="font-size:120%;">{{this.description1}}</p>
            <a href="{{this.link1}}">
            <button class="btn btn-info"> {{this.linktext1}} </button>
            </a>
          </div>
          <div class="col-md-6 text-center py-3">
          <img src="{{this.imageUrl1}}" class="img-fluid" alt="" style="height:274px;"/>
          </div>
        </div>
        {{/each}}
      </div>
     </section>

<section class="banner3_section py-5">
      <div class="container">
        <div class="row ">
          {{#each bannera}}
          <div class="col-md-6 text-center py-4 ">
          <img src="{{this.imageUrl2}}" class="img-fluid" alt="" style="height:274px;"/>
          </div>
          <div class="col-md-6 text-center pt-4">
            <h2>
            {{this.title2}}
            </h2>
            <p style="font-size:120%;" >{{this.description2}}</p>
            <a href="this.imageUrl2">
            <button class="btn btn-info">{{this.linktext2}}</button>
            </a>
          </div>
          
        </div>
        {{/each}}
      </div>
     </section>

    {{!-- contact us section --}}
    <section id="contact-us-section" class="contact_form ">
      <div class="container " >
        <div class="row">
          <div class="col-md-12">
            <!-- Default form contact -->
<form class=" border p-5 bg-images " action="/process-contact-form" method="post">

    <p class="h4 mb-4 contact_head">Contact us</p>

    <!-- Name -->
    <input name="name" type="text" id="defaultContactFormName" class="form-control mb-4 w-50" placeholder="Name">

    <!-- Email -->
    <input name="email" type="email" id="defaultContactFormEmail" class="form-control mb-4 w-50" placeholder="E-mail">

    <!-- Subject -->
    <label>Subject</label>
    <select class="browser-default custom-select mb-4 w-25">
        <option value="" disabled>Choose option</option>
        <option value="1" selected>Feedback</option>
        <option value="2">Report a bug</option>
        <option value="3">Feature request</option>
        <option value="4">Feature request</option>
    </select>

    <!-- Message -->
    <div class="form-group">
        <textarea name="message" class="form-control rounded-0 w-50" id="exampleFormControlTextarea2" rows="3" placeholder="Message"></textarea>
    </div>

    <!-- Copy -->
    <div class="custom-control custom-checkbox mb-4 ">
        <input  type="checkbox" class="custom-control-input" id="defaultContactFormCopy">
        <label class="custom-control-label" for="defaultContactFormCopy">Send me a copy of this message</label>
    </div>

    <!-- Send button -->
    <button class="btn btn-info btn-block button_space" type="submit">Send</button>

</form>
<!-- Default form contact -->
          </div>
        </div>
      </div>
    </section>
<section class="about_section py-5 bg-color">
  <div class="container text-center">
    <div class="row">
      {{#each abouts}}
      <div class="col-md-12">
        <h2>{{this.title3}}</h2>
        <p style="font-size:120%;">
                 {{this.description3}}
        </p>
        <a href="{{this.link3}}">
        <button class="btn btn-success"  >
          
          {{this.linktext3}}</button>
          </a>
      </div>
    </div>
    {{/each}}
  </div>

</section>
<footer class="footer text-light">
  <div class="container">
    {{#each foot}}
    <div class="col-md-12">
      <div class="text-center p-4">
        {{this.title4}}
        <a class="text-reset fw-bold" href="{{this.link4}}">{{this.linktext4}}</a>
      </div>
    </div>
    {{/each}}
  </div>
</footer>




   
      </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

  </body>
</html>